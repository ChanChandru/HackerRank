emp=[10,20,30,40,50]
man=[20,50,20,50,50]
manager=set(man);two=[]
manager=list(manager)
for i in range(0,len(manager)):
  l=[]
  for j in range(0,len(man)):
    if(man[j]==manager[i] and man[j]!=emp[j]):
      l.append(emp[j])
  two.append(list(l))
for i in range(0,len(manager)):
    print(manager[i],"---->",two[i])
